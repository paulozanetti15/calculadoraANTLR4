{
  "name": "calculadora-antlr",
  "main": "main.ts",
  "dependencies": {
    "antlr4ts": "^0.5.0-alpha.4",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "fs": "^0.0.1-security",
    "typescript": "^5.4.3"
  },
  "scripts": {
    "antlr4ts": "antlr4ts -visitor -o ./antlrFiles CalculadoraANTLR.g4",
    "build": "tsc -p ./tsconfig.json && node build/main.ts",
    "start": "npm run antlr4ts && npm run build && node build/main.ts",
    "evaluate": "npm run build && node build/main.ts"
  },
  "devDependencies": {
    "@types/node": "^20.12.2"
  }
}
